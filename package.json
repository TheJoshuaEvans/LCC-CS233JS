{
  "name": "lcc-cs233js",
  "version": "1.0.0",
  "description": "Resources for the LCC CS 233 JS (Intermediate JavaScript) course. Taken Spring term 2025",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "webpack": "webpack",
    "webpack:bookmarker:build": "npm run webpack -- --config webpack/bookmarker.webpack.js",
    "webpack:bookmarker:watch": "npm run webpack -- serve --config webpack/bookmarker.webpack.js --open",
    "webpack:todolist:build": "npm run webpack -- --config webpack/todolist.webpack.js",
    "webpack:todolist:watch": "npm run webpack -- serve --config webpack/todolist.webpack.js --open",
    "webpack:meme:build": "npm run webpack -- --config webpack/meme.webpack.js",
    "webpack:meme:watch": "npm run webpack -- serve --config webpack/meme.webpack.js --open",
    "webpack:bookmarker2:build": "npm run webpack -- --config webpack/bookmarker2.webpack.js",
    "webpack:bookmarker2:watch": "npm run webpack -- serve --config webpack/bookmarker2.webpack.js --open",
    "webpack:weather:build": "npm run webpack -- --config webpack/weather.webpack.js",
    "webpack:weather:watch": "npm run webpack -- serve --config webpack/weather.webpack.js --open"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TheJoshuaEvans/LCC-CS233JS.git"
