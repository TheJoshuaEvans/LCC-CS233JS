    "@stylistic/eslint-plugin-js": "^4.2.0",
    "@types/jest": "^29.5.14",
    "babel-loader": "^9.2.1",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^7.1.2",
    "dotenv": "^16.5.0",
    "eslint": "^9.23.0",
