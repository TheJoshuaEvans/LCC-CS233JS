{
  "name": "lcc-cs233js",
  "version": "1.0.0",
  "description": "Resources for the LCC CS 233 JS (Intermediate JavaScript) course. Taken Spring term 2025",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "server:json": "json-server --watch data/db.json",
