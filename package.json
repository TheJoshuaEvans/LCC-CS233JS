    "webpack:bookmarker2:watch": "npm run webpack -- serve --config webpack/bookmarker2.webpack.js --open",
    "webpack:weather:build": "npm run webpack -- --config webpack/weather.webpack.js",
    "webpack:weather:watch": "npm run webpack -- serve --config webpack/weather.webpack.js --open",
    "webpack:event:build": "npm run webpack -- --config webpack/event.webpack.js",
    "webpack:event:watch": "npm run webpack -- serve --config webpack/event.webpack.js --open"
