    "eslint": "^9.23.0",
    "jest": "^29.7.0",
    "babel-loader": "^9.2.1",
    "@babel/cli": "^7.26.4",
    "@babel/core": "^7.26.8",
    "@babel/preset-env": "^7.26.8",
    "html-webpack-plugin": "^5.6.3",
    "copy-webpack-plugin": "^12.0.2",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.0"
  },
  "jest": {
    "transform": {}
  }
